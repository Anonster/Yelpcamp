<%- include('Partials/header')  %>
<div class="container">
    <div class="row">
        <div class="col">
            <h1 class="text-center mt-4">Login </h1>
        </div>
    </div>
    <div class="row">
        <div class="col">
            <form class="myCustom" action="/login" method="POST">
                <div class="form-group col-lg-5 col-md-8 m-auto">
                    <label for="username">Username</label>
                    <input class="form-control form-control-lg" type="text" placeholder="Username" name="username">
                </div>
                <div class="form-group col-lg-5 col-md-8 pt-3 m-auto">
                    <label for="password">Password</label>
                    <input class="form-control form-control-lg" type="password" name="password" placeholder="Password">
                </div>
                <div class="form-group col-md-5 pt-3 m-auto">
                    <button class="btn btn-block btn-primary" type="submit">Submit</button>
                </div>

                <div class="form-group col-md-5 pt-3 m-auto">
                    <a href="/">Go Back</a>
                </div>
            </form>
        </div>
    </div>
</div>
<%- include('Partials/footer')  %>